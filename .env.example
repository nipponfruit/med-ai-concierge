# Server
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# Optional: allow only a specific origin (e.g. https://your-frontend.example)
CORS_ORIGIN=

# Rate limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=60

# Optional: Embeddings (if provided, uses OpenAI embeddings; otherwise TF-IDF fallback)
OPENAI_API_KEY=
