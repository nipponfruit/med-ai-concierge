{
  "version": 2,
  "routes": [
    { "src": "^/health$", "dest": "/api/health" },
    { "src": "^/kb/(.*)$", "dest": "/api/kb/$1" },
    { "src": "^/$", "dest": "/public/index.html" },
    { "src": "^/(style\\.css|app\\.js)$", "dest": "/public/$1" },
    { "src": "^/(.*\\.(css|js|png|jpg|jpeg|svg|ico|webp))$", "dest": "/public/$1" }
  ]
}
